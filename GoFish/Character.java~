public class Character {
  
  private String name;
  private int level;
  
  public Character (String name) {
    this.name = name;
  }
  
  public void reName (String newName) {
    this.name = newName;
  }
  
  public String getName () {
    return name;
  }
  
  public void levelUp () {
    level++;
  }
  
  public int getLevel () {
    return level;
  }
}